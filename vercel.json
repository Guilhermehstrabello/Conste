{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/nps",
      "has": [
        {
          "type": "host",
          "value": "nps.constemarketing.com.br"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/nps",
      "destination": "https://nps.constemarketing.com.br",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "constemarketing.com.br"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
